{
    "author": "OpenVidu",
    "name": "openvidu-react",
    "version": "2.17.0",
    "description": "OpenVidu React",
    "repository": {
        "type": "git",
        "url": "git://github.com/OpenVidu/openvidu-call-react"
    },
    "keywords": [
        "streaming",
        "webrtc",
        "openvidu",
        "media"
    ],
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "scripts": {
        "start": "react-scripts start",
        "test": "react-scripts test --env=jsdom",
        "build": "rm -rf dist && cross-env NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
        "create-library": "npm run build && npm pack",
        "eject": "react-scripts eject"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "dependencies": {
        "@material-ui/core": "4.11.0",
        "@material-ui/icons": "4.9.1",
        "axios": "0.20.0",
        "jquery": "3.5.1",
        "openvidu-browser": "2.17.0",
        "typeface-roboto": "0.0.75"
    },
    "devDependencies": {
        "@babel/cli": "7.10.5",
        "@babel/core": "7.11.4",
        "@babel/preset-react": "7.10.4",
        "cross-env": "^7.0.3",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-scripts": "3.4.3"
    }
}
